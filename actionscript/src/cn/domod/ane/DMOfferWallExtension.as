package cn.domod.ane{	import flash.events.EventDispatcher;	import flash.events.StatusEvent;	import flash.external.ExtensionContext;
		/**	 * 多盟积分墙	 * 请加入以下东东	 * <uses-permission android:name="android.permission.INTERNET"/><uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/> <uses-permission android:name="android.permission.READ_PHONE_STATE"/> <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/> <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/> <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/> <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/> <activity android:name="cn.domob.download.OActivity”/>	 * 	 * @author czq	 * 	 */		public class DMOfferWallExtension extends EventDispatcher	{				private static var _extContext:ExtensionContext;				private static var EXTENSION_ID:String = 'cn.domod.dmofferwall.extension';				//与java端中Map里的key一致		private static const DMINIT:String = "DMInit";		private static const DMCONSUME:String = "DMConsume";		private static const DMCHECKSOCRE:String = "DMcheckSocre";		private static const DMCHECKSTATUS:String = "DMcheckStatus";		private static const DMSHOWOFFERWALL:String = "DMShowOfferWall";		private static const DMSHOWVIDEOOFFERWALL:String = "DMShowVideoOfferWall";		private static const DMLOADINTERSTITIALOFFERWALL:String = "DMLoadInterstitialOfferWall";		private static const DMSHOWINTERSTITIALOFFERWALL:String = "DMShowInterstitialOfferWall";		public function DMOfferWallExtension()		{					}				private static var _instance:DMOfferWallExtension;				public static function getInstance():DMOfferWallExtension		{			if (!_instance)			{				_instance = new DMOfferWallExtension();				_extContext=ExtensionContext.createExtensionContext(EXTENSION_ID, null);				_extContext.addEventListener(StatusEvent.STATUS, onStatus);				if (!_extContext)				{					trace("extension.xml出错啦！！！！请对应上EXTENSION_ID是否与extenion.xml里的id想同");				}				else				{					trace("多盟积分墙extension创建成功");				}			}			return _instance;		}				private static function onStatus(e:StatusEvent):void		{			_instance.dispatchEvent(e);		}				public static function DMInit(publisherId:String):void		{			if (_extContext){				_extContext.call(DMINIT, publisherId);			}		}				public static function DMConsume(point:String):void		{			if (_extContext){				_extContext.call(DMCONSUME, point);			}		}				public static function DMcheckSocre():void		{			if (_extContext){				_extContext.call(DMCHECKSOCRE);			}		}				public static function DMcheckStatus():void		{			if (_extContext){				_extContext.call(DMCHECKSTATUS);			}		}				public static function DMShowOfferWall():void		{			if (_extContext){				_extContext.call(DMSHOWOFFERWALL);			}		}				public static function DMShowVideoOfferWall():void		{			if (_extContext){				_extContext.call(DMSHOWVIDEOOFFERWALL);			}		}				public static function DMLoadInterstitialOfferWall():void		{			if (_extContext){				_extContext.call(DMLOADINTERSTITIALOFFERWALL);			}		}				public static function DMShowInterstitialOfferWall():void		{			if (_extContext){				_extContext.call(DMSHOWINTERSTITIALOFFERWALL);			}		}	}}